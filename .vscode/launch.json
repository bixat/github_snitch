	"version": "0.2.0",
	"configurations": [
		{
			"name": "example",
			"cwd": "example",
			"request": "launch",
			"type": "dart",
			//TODO: Add your keys here
			"args": [
				"--dart-define",
				"owner=<owner>",
				"--dart-define",
				"repo=<repo>",
				"--dart-define",
				"token=<token>"
			]
		},
		{
			"name": "example (profile mode)",
			"cwd": "example",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile",
			"args": [
				"--dart-define owner=owner",
				"--dart-define repo=repo",
				"--dart-define token=token"
			]
		},
		{
			"name": "example (release mode)",
			"cwd": "example",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release",
			"args": [
				"--dart-define owner=owner",
				"--dart-define repo=repo",
				"--dart-define token=token"
			]
		}
	]
